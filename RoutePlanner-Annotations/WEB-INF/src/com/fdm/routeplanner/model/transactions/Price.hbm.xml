<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.fdm.routeplanner.model.transactions.Price"
		table="RPPrices">
		<id column="zonenumber" name="zone" />
		<property column="juniorcost" generated="never" lazy="false"
			name="junior" />
		<property column="adultcost" generated="never" lazy="false"
			name="adult" />
		<property column="seniorcost" generated="never" lazy="false"
			name="senior" />
	</class>
</hibernate-mapping>
